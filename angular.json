{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shell": {
      "projectType": "application",
      "schematics": {
				"@schematics/angular:application": {
					"strict": true
				},
        "@schematics/angular:component": {
          "style": "scss"
        }
			},
      "root": "projects",
      "sourceRoot": "projects/shell/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/shell",
            "index": "projects/shell/src/index.html",
            "main": "projects/shell/src/main.ts",
            "polyfills": "projects/shell/src/polyfills.ts",
            "tsConfig": "projects/shell/tsconfig.app.json",
            "assets": [
              "projects/shell/src/favicon.ico",
              "projects/shell/src/assets"
            ],
            "styles": [
              "projects/shell/src/styles.scss"
            ],
            "scripts": [],
            "extraWebpackConfig": "projects/shell/webpack.config.js"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/shell/src/environments/environment.ts",
                  "with": "projects/shell/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "projects/shell/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "shell:build:production",
              "extraWebpackConfig": "projects/shell/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "shell:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
						"publicHost": "http://localhost:4200",
						"port": 4200,
						"extraWebpackConfig": "projects/shell/webpack.config.js"
					}
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "shell:build",
            "extraWebpackConfig": "projects/shell/webpack.config.js"
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "projects/shell/src/test.ts",
            "polyfills": "projects/shell/src/polyfills.ts",
            "tsConfig": "projects/shell/tsconfig.spec.json",
            "karmaConfig": "projects/shell/karma.conf.js",
            "assets": [
              "projects/shell/src/favicon.ico",
              "projects/shell/src/assets"
            ],
            "styles": [
              "projects/shell/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "remote-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/remote-app",
      "sourceRoot": "projects/remote-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/remote-app",
            "index": "projects/remote-app/src/index.html",
            "main": "projects/remote-app/src/main.ts",
            "polyfills": "projects/remote-app/src/polyfills.ts",
            "tsConfig": "projects/remote-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/remote-app/src/favicon.ico",
              "projects/remote-app/src/assets"
            ],
            "styles": [
              "projects/remote-app/src/styles.scss"
            ],
            "scripts": [],
            "extraWebpackConfig": "projects/remote-app/webpack.config.js"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/remote-app/src/environments/environment.ts",
                  "with": "projects/remote-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "projects/remote-app/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "remote-app:build:production",
              "extraWebpackConfig": "projects/remote-app/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "remote-app:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
						"publicHost": "http://localhost:5000",
						"port": 5000,
						"extraWebpackConfig": "projects/remote-app/webpack.config.js"
					}
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "remote-app:build",
            "extraWebpackConfig": "projects/remote-app/webpack.config.js"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/remote-app/src/test.ts",
            "polyfills": "projects/remote-app/src/polyfills.ts",
            "tsConfig": "projects/remote-app/tsconfig.spec.json",
            "karmaConfig": "projects/remote-app/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/remote-app/src/favicon.ico",
              "projects/remote-app/src/assets"
            ],
            "styles": [
              "projects/remote-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}